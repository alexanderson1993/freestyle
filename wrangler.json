{
  "$schema": "https://unpkg.com/wrangler@latest/config-schema.json",
  "name": "freestyle",
  "main": "./app/entry.worker.ts",
  "compatibility_date": "2025-02-04",
  "compatibility_flags": ["nodejs_compat"],
  "workers_dev": true,
  "assets": {
    "directory": "./build/client/"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "freestyle",
      "database_id": "aea718fc-b2ba-4b70-8a9e-3f988b3daa87"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "Server",
        "name": "Server"
      }
    ]
  },
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "31d732739af14a39ace310fc19ab7239"
    }
  ],
  "r2_buckets": [
    {
      "bucket_name": "freestyle",
      "binding": "R2"
    }
  ],
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Server"]
    }
  ],
  "env": {
    "development": {
      "vars": {
        "ENV": "development"
      }
    },
    "production": {
      "vars": {
        "ENV": "production"
      }
    }
  }
}
