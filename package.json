{
  "name": "cloudflare-middleware",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "deploy": "npm run build && wrangler deploy",
    "dev": "vite dev",
    "start": "vite preview",
    "typecheck": "react-router typegen && tsc -b",
    "types": "wrangler types",
    "sly": "bunx @sly-cli/sly add",
    "db:migrate:auth": "bun run scripts/migrate.ts",
    "db:migrate:create": "wrangler d1 migrations create freestyle",
    "db:migrate": "wrangler d1 migrations apply freestyle",
    "db:generate": "bunx --bun kysely-codegen --url $(find ./.wrangler/state/v3/d1/miniflare-D1DatabaseObject -name \"*.sqlite\" | head -n 1) --dialect kysely-bun-sqlite"
  },
  "dependencies": {
    "@conform-to/react": "^1.2.2",
    "@conform-to/zod": "^1.2.2",
    "@edgefirst-dev/r2-file-storage": "^1.0.2",
    "@edgefirst-dev/server-timing": "^0.0.1",
    "@epic-web/cachified": "^5.5.1",
    "@epic-web/client-hints": "^1.3.5",
    "@epic-web/invariant": "^1.0.0",
    "@mjackson/form-data-parser": "^0.7.0",
    "better-auth": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "isbot": "^5.1.17",
    "kysely": "^0.27.6",
    "kysely-d1": "^0.3.0",
    "partyserver": "^0.0.65",
    "partysocket": "^1.0.3",
    "react": "^19.0.0",
    "react-aria-components": "^1.7.1",
    "react-dom": "^19.0.0",
    "react-router": "^7.3.0",
    "remix-utils": "^8.4.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-react-aria-components": "^2.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^0.1.10",
    "@react-router/dev": "^7.3.0",
    "@sly-cli/sly": "^1.14.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/bun": "^1.2.5",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "kysely-bun-sqlite": "^0.3.2",
    "kysely-codegen": "^0.18.3",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.2.1",
    "vite-plugin-icons-spritesheet": "^3.0.1",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "4"
  }
}
